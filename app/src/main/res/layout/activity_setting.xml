<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitXY"
        android:src="@drawable/more_screen_bg_blur" />

    <RelativeLayout
        android:id="@id/topLayout"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/phone_top_bar_height"
        android:layout_alignParentTop="true"
        android:background="@color/top_bar_background">

        <TextView
            android:id="@id/topHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="الإعدادات"
            android:textColor="#ffffffff"
            android:textSize="@dimen/heading_phone" />
    </RelativeLayout>

    <ScrollView
        android:id="@id/scrollView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/topLayout"
        android:overScrollMode="never"
        android:scrollbars="none">

        <RelativeLayout
            android:id="@id/subRelative"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/scrollView">

            <View
                android:id="@id/firstSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/PregnancySettingLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/AppSettingLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/firstSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:src="@drawable/app_setting_icon" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid">

                    <TextView
                        android:id="@id/appSettingtitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical"
                        android:text="إعدادات البرنامج"
                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/secondSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/AppSettingLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/AccountSettingLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/secondSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:src="@drawable/account_setting_icon" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid">

                    <TextView
                        android:id="@id/accSettingstitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"

                        android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:text="إعدادات الحساب"
                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/thirdSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/AccountSettingLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/tellAFriendLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/thirdSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:paddingLeft="3.0dip"
                    android:src="@drawable/export_data" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid"
                    android:paddingLeft="3.0dip">

                    <TextView
                        android:id="@id/tellFriendTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical"
                        android:text="أخبر صديق"
                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/fourthSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/tellAFriendLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/editProfileLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/fourthSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:src="@drawable/otherapps_icon" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"

                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid">

                    <TextView
                        android:id="@id/editProfileTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical"
                        android:text="برامج أخرى"

                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/sixthSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/editProfileLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/aboutUsLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/sixthSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:src="@drawable/about_us_icon" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid">

                    <TextView
                        android:id="@id/aboutUsTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:text="من نحن ؟"
                        android:layout_alignParentRight="true"

                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/seventhSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/aboutUsLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/faqLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/seventhSeparator"
                android:background="@drawable/email_input"
                android:padding="5.0dip">

                <ImageView
                    android:id="@id/third_imgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:src="@drawable/faq_icon" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5.0dip"
                    android:layout_toRightOf="@id/disclosuretriangle"
                    android:layout_toLeftOf="@id/third_imgid">

                    <TextView
                        android:id="@id/faqTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:layout_alignParentRight="true"

                        android:text="تعليمات"
                        android:textColor="#ff5a5a5a"
                        android:textSize="21.0sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@id/disclosuretriangle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/discloure" />
            </RelativeLayout>

            <View
                android:id="@id/eightSeparator"
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:layout_below="@id/faqLayout"
                android:background="@android:color/darker_gray" />

            <RelativeLayout
                android:id="@id/tabWidgetLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/eightSeparator">

                <TabWidget
                    android:id="@android:id/tabs"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="12.0dip"
                    android:layout_weight="0.0"
                    android:background="@drawable/square"
                    android:orientation="horizontal"
                    android:showDividers="end"
                    android:tabStripEnabled="false"
                    android:visibility="invisible" />
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>